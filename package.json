{
  "name": "ditch-typescript-adopt-jsdoc",
  "version": "0.1.0",
  "private": false,
  "description": "A sample React app with a bunch of JSDoc comments which should allow us to generate \"d.ts\" files",
  "main": "dist/index.html",
  "scripts": {
    "eslint": "eslint --ext js,jsx src/",
    "eslint:fix": "eslint --fix --ext js,jsx src/",
    "prettier": "prettier --write \"*.{js,jsx,md,json}\" \"src/*.{js,jsx}\"",
    "lint": "$npm_execpath run eslint:fix && $npm_execpath run prettier",
    "type:check": "tsc -p tsconfig.json",
    "type:generate": "rimraf types && tsc -p tsconfig.generate.json",
    "jsdoc": "rimraf jsdoc && jsdoc -c jsdoc.conf.js -d jsdoc/",
    "build:clean": "rimraf dist",
    "build:dev": "$npm_execpath run build:clean && webpack serve --config webpack.dev.js",
    "build": "$npm_execpath run build:clean && webpack --config webpack.prod.js",
    "start": "$npm_execpath run build:dev"
  },
  "repository": "https://github.com/minagawah/ditch-typescript-adopt-jsdoc.git",
  "keywords": [
    "JSDoc",
    "React"
  ],
  "author": "minagawah <minag@tokyo800.jp>",
  "license": "(Unlicense OR MIT)",
  "bugs": {
    "url": "https://github.com/minagawah/ditch-typescript-adopt-jsdoc/issues"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "core-js": "^3.34.0",
    "history": "^5.3.0",
    "i18next": "^23.7.8",
    "i18next-browser-languagedetector": "^7.2.0",
    "js-cookie": "^3.0.5",
    "moment": "^2.29.4",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-responsive-carousel": "^3.2.23",
    "react-router": "^6.20.1",
    "react-router-dom": "^6.20.1",
    "sanitize-html": "^2.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.2.1",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/sanitize-html": "^2.9.5",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-preval": "^5.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.4",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "jsdoc-tsimport-plugin": "^1.0.5",
    "license-webpack-plugin": "^4.0.2",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.2",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.6",
    "twin.macro": "^3.4.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
